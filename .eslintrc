{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "ecmaFeatures": {
        "arrowFunctions": true,
        "binaryLiterals": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "modules": true,
        "objectLiteralComputedProperties": true,
        "restParams": true,
        "spread": true,
        "templateStrings": true
    },
    "env": {
        "browser": true
    },
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "none"
            }
        ],
        "array-bracket-spacing": 2,
        "comma-style": 2,
        "object-curly-spacing": 2,
        "operator-linebreak": 2,
        "quotes": [
            2,
            "double"
        ],
        "space-before-blocks": 2,
        "space-in-parens": 2,
        "space-unary-ops": 2,
        "no-use-before-define": 0,
        "new-cap": 0,
        "no-underscore-dangle": 0,
        "no-extra-strict": 0,
        "eol-last": 0,
        "no-shadow": 0,
        "yoda": 0,
        "no-console": 0,
        "no-extend-native": 0
    }
}